public class Shop {
  private int costOfEnergy = 10;
  private int energyIncrease = 20;
  private int costOfHouse = 5000;
  private int costOfHouses= 10000;


  //Mutators all programatically controlled

  public int getCostOfEnergy() {
    return costOfEnergy;
  }
  public int getCostOfHouse() {
    return costOfHouse;
  }

  public int getEnergyIncrease() {
    return energyIncrease;
  }
  public int getCostOfHouses() {
    return costOfHouses;
  }
}
